<h1>Add your informations</h1>

<%= simple_form_for @profile do |f| %>
    <%= f.input :firstname %>
    <%= f.input :lastname %>
    <%= f.input :username %>
    <%= f.date_field :birthdate %>
    <%= f.input :city %>
    <%= f.input :photo, as: :file %>
    <div class="form-actions">
      <%= f.submit %>
    </div>
  <% end %>